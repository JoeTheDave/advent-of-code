{
  "name": "advent-of-code",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run tailwind -- --watch",
    "dev:vite": "vite",
    "build": "run-s \"build:*\"",
    "build:css": "npm run tailwind -- --minify",
    "build:vite": "tsc && vite build",
    "preview": "vite preview",
    "tailwind": "npx tailwindcss -i ./lib/styles/index.css -o ./lib/styles/tailwind.css",
    "exec": "esno ./scripts/exec.ts",
    "generate": "esno ./scripts/generate.ts"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.1",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "esno": "^4.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
