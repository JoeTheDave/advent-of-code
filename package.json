{
  "name": "advent-of-code-js",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run tailwind -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:css": "npm run tailwind -- --watch",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.ts --watch ./server.ts",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.ts",
    "tailwind": "npx tailwindcss -i ./app/styles/index.css -o ./app/styles/tailwind.css",
    "typecheck": "tsc -b",
    "generate-solution-files": "ts-node ./scripts/generate-solution-files.ts",
    "exec": "ts-node ./scripts/exec-solution.ts",
    "delete-default-solutions": "ts-node ./scripts/delete-default-solutions.ts",
    "delete-solution": "ts-node ./scripts/delete-solution.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-hook/window-size": "^3.1.1",
    "@remix-run/express": "^1.8.2",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "axios": "^1.2.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.15",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "tailwindcss-textshadow": "^2.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
